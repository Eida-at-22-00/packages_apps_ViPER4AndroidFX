android_app_import {
    name: "ViPER4AndroidFX",
    owner: "ViperFX",
    apk: "system-ext/app/ViPER4AndroidFX/ViPER4AndroidFX.apk",
    preprocessed: true,
    presigned: true,
    dex_preopt: {
        enabled: false,
    },
    overrides: ["AudioFX"],
    system_ext_specific: true,
}

cc_prebuilt_library_shared {
    name: "libv4a_re",
    strip: {
        none: true,
    },
    target: {
        android_arm: {
            srcs: [
                "vendor/lib/soundfx/libv4a_re.so",
            ],
        },
        android_arm64: {
            srcs: [
                "vendor/lib64/soundfx/libv4a_re.so",
            ],
        },
    },
    shared_libs: [
        "libaudioutils",
        "libcutils",
        "liblog",
        "libc++",
        "libc",
        "libm",
        "libdl",
    ],
    compile_multilib: "both",
    relative_install_path: "soundfx",
    prefer: true,
    soc_specific: true,
}
